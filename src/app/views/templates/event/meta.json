{
    "name": {
        "name"      : "Event name",
        "type"      : "text",
        "default"   : "Event name"
    },
    "tagline": {
        "name"      : "Tagline",
        "type"      : "text",
        "default"   : "Event's Tagline",
        "optional"  : true
    },
    "description": {
        "name"      : "Description",
        "type"      : "longtext",
        "default"   : "Event details coming soon."
    },
    "rules": {
        "name"      : "Rules",
        "type"      : "list",
        "listType"  : "text"
    },
    "status": {
        "name"      : "Status",
        "type"      : "text",
        "optional"  : true
    },
    "button_link": {
        "name"      : "Button link (absolute link or path [e.g. threads/codecraft/register/] [recommended])",
        "type"      : "text",
        "optional"  : true
    },
    "button_text": {
        "name"      : "Button text",
        "type"      : "text",
        "default"   : "",
        "optional"  : true
    },
    "start_time": {
        "name"      : "Start_time",
        "type"      : "datetime",
        "default"   : "To be announced",
        "optional"  : true
    },
    "end_time": {
        "name"      : "End_time",
        "type"      : "datetime",
        "default"   : "To be announced",
        "optional"  : true
    },
    "venue": {
        "name"      : "Venue",
        "type"      : "text",
        "default"   : "Online",
        "optional"  : true
    },
    "contact_email": {
        "name"      : "Contact email",
        "type"      : "text",
        "default"   : "contact@felicity.iiit.ac.in",
        "optional"  : true
    },
    "og_image": {
        "name"      : "Image link to be displayed when page shared on fb (Only suffix, baseUrl already there)",
        "type"      : "text",
        "default"   : "files/16/logos/felicity16-logo-large.png",
        "optional"  : true
    },
    "events_data": {
        "name"      : "Events list",
        "inApi"     : false,
        "type"      : "external",
        "path"      : ".*/.*",
        "template"  : "(category|event)",
        "data"      : {
            "name" : "name",
            "icon" : "nav_icon"
        }
    }
}
